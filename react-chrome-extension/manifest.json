{
    "manifest_version": 3,
    "name": "HistoriNaija",
    "version": "1.0.0",
    "permissions": ["scripting", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_title": "Google AI Challenge"
    }
  }
  